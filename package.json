{
  "name": "typescript-api-template",
  "version": "1.0.0",
  "description": "A simple template generic TS API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon -r --exec \"npm run start\" --watch src --ext ts",
    "watch": "ts-node-dev -r tsconfig-paths/register src/watch.ts",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "template",
    "typescript",
    "api"
  ],
  "author": "gabriel-roque <https://github.com/gabriel-roque>",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-mung": "^0.5.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mung": "^0.5.1",
    "module-alias": "^2.2.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "nodemon": "^2.0.4",
    "ts-node-dev": "^1.0.0-pre.50",
    "tsconfig-paths": "^3.9.0"
  },
  "_moduleAliases": {
    "@api": "dist/config/api",
    "@routes": "dist/config/routes/routes",
    "@server": "dist/config/server",
    "@controllers": "dist/app/controllers",
    "@middlewares": "dist/app/middlewares"
  }
}
